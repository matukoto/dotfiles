{
  "coc.preferences.formatOnSaveFiletypes": [
    "vue",
    "tsx",
    "ts",
    "go",
    "md",
    // "yaml",
    "json",
    "vim",
    "lua"
  ],
  "languageserver": {
    "efm": {
      "command": "efm-langserver",
      "args": ["-c", "$HOME/.config/efm-langserver/config.yaml"],
      "filetypes": ["markdown, vim"]
    }
  }
}
