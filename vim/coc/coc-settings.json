{
  "diagnostic.enableHighlightLineNumber": false,
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.errorSign": "",
  "coc.preferences.formatOnSaveFiletypes": [
    "vue",
    "tsx",
    "ts",
    "go",
    "yaml",
    "json",
    "vim",
    "lua",
    "Dockerfile",
    "java",
    "svelte"
  ],
  "suggest.invalidInsertCharacters": [],
  "languageserver": {
    "bufls": {
      "command": "bufls",
      "rootPatterns": ["buf.yaml"],
      "args": ["serve"],
      "filetypes": ["proto"]
    },
    "lua": {
      "command": "~/.local/share/lua-language-server/bin/lua-language-server",
      "args": ["-E", "~/.local/share/lua-language-server/bin/main.lua"],
      "filetypes": ["lua"],
      "settings": {
      "Lua": {
        "diagnostics": {
          "globals": ["vim"]
        },
        "workspace": {
          "library": {
            ["/usr/share/awesome/lib"]: true,
            ["/usr/share/awesome/lib/"]: true,
            ["/usr/share/awesome/lib/?.lua"]: true,
            ["/usr/share/awesome/lib/?/init.lua"]: true
          },
        },
      }
    }
  },
  "list.source.symbols.excludes": ["**/node_modules/**"],
  "go.goplsOptions": {
    "staticcheck": true
  },
  "suggest.floatConfig": {
    "border": true,
    "rounded": true,
    "borderhighlight": "Grey"
  },
  "floatFactory.floatConfig": {
    "border": true,
    "rounded": true,
    "borderhighlight": "Grey"
  },
  "suggest.completionItemKindLabels": {
    "function": "󰊕 ",
    "variable": " ",
    "module": "󰕳 ",
    "snippet": " ",
    "struct": " ",
    "keyword": " ",
    "property": " ",
    "method": " ",
    "field": " ",
    "interface": " ",
    "folder": " ",
    "file": " ",
    "class": " ",
    "constant": " "
  },
  "snippets.ultisnips.pythonPrompt": false,
  "svelte.enable-ts-plugin": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "Lua.diagnostics.globals": ["vim"]
}
