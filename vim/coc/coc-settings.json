{
  "diagnostic.enableHighlightLineNumber": false,
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.errorSign": "",
  "coc.preferences.formatOnSaveFiletypes": [
    "vue",
    "tsx",
    "ts",
    "go",
    "yaml",
    "json",
    "vim",
    "lua",
    "Dockerfile",
    "java",
    "svelte"
  ],
  "suggest.invalidInsertCharacters": [],
  "languageserver": {
    "efm": {
      "command": "efm-langserver",
      "args": [],
      "filetypes": ["markdown", "vim", "md", "vimwiki"]
    },
    "zk": {
      "command": "zk",
      "args": ["lsp", "--log", "/tmp/zk-lsp.log"],
      "trace.server": "verbose",
      "filetypes": ["vimwiki", "markdown"]
    },
    "bufls": {
      "command": "bufls",
      "rootPatterns": ["buf.yaml"],
      "args": ["serve"],
      "filetypes": ["proto"]
    }
  },
  "list.source.symbols.excludes": ["**/node_modules/**"],
  "go.goplsOptions": {
    "staticcheck": true
  },
  "suggest.floatConfig": {
    "border": true,
    "rounded": true,
    "borderhighlight": "Grey"
  },
  "floatFactory.floatConfig": {
    "border": true,
    "rounded": true,
    "borderhighlight": "Grey"
  },
  "suggest.completionItemKindLabels": {
    "function": "󰊕 ",
    "variable": " ",
    "module": "󰕳 ",
    "snippet": " ",
    "struct": " ",
    "keyword": " ",
    "property": " ",
    "method": " ",
    "field": " ",
    "interface": " ",
    "folder": " ",
    "file": " ",
    "class": " ",
    "constant": " "
  },
  "snippets.ultisnips.pythonPrompt": false,
  "svelte.enable-ts-plugin": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "Lua.diagnostics.globals": ["vim"]
}
